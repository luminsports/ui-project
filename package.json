    "typecheck": "vue-tsc --noEmit --composite false"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@rollup/plugin-typescript": "^8.3.2",
    "@storybook/addon-actions": "^7.0.8",
