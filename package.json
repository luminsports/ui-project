    "@storybook/addon-interactions": "^7.0.8",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/vue3": "^7.0.8",
    "@storybook/vue3-vite": "^7.0.8",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.3.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "c8": "^7.11.3",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.8",
    "tailwindcss": "^3.1.5",
